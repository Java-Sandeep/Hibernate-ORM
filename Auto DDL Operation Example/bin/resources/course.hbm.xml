<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="beans.Course"  table="course" schema="test">
		<id name="cid"/>
		<property name="cname"/>
		<property name="fee" column="cfee"/>
		<property name="session" column="csession"/>
		<property name="duration" column="cduration"/>
		<property name="syllabus" column="csyllabus"/>
	</class>
</hibernate-mapping>