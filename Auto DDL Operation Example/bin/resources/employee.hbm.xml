<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="beans.Employee" table = "employee" schema="test">
		<id name="eid"/>
		<property name="name" column="ename"/>
		<property name="email" column="eemail"/>
		<property name="location" column="elocation"/>
	</class>
</hibernate-mapping>